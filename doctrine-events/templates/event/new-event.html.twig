{% extends 'base.html.twig' %}

{% block title %}Nouvel événement{% endblock %}

{% block body %}
<div class="container mt-5">
    <h1>Création d'événement</h1>
    <form method="post">
        {{ form_start(form) }}
        {{ form_widget(form) }}

        <button type="submit" class="btn btn-primary">Créer</button>
        {{ form_end(form) }}
    </form>
</div>
{% endblock %}